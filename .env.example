VITE_APP_NAME="${APP_NAME}"

# ============================================
# ISP BILLING INTEGRATION SETTINGS
# ============================================

# Mikrotik RouterOS API
MIKROTIK_ENABLED=false
MIKROTIK_HOST=192.168.1.1
MIKROTIK_PORT=8728
