# Xendit Payment Gateway
XENDIT_SECRET_KEY=
XENDIT_CALLBACK_TOKEN=

# Default Payment Gateway (midtrans/xendit)
PAYMENT_DEFAULT_GATEWAY=midtrans

# ============================================
# RADIUS SERVER INTEGRATION
# ============================================
